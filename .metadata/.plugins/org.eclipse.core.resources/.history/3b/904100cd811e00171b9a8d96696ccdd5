package week3;

public class Point {
	public int xPoint;
	public int yPoint;
	
	public Point(int x,int y)
	{
		this.xPoint = x;
		this.yPoint = y;
	}
	public int getxPoint() {
		return xPoint;
	}
	public void setxPoint(int xPoint) {
		this.xPoint = xPoint;
	}
	public int getyPoint() {
		return yPoint;
	}
	public void setyPoint(int yPoint) {
		this.yPoint = yPoint;
	}
	

}
